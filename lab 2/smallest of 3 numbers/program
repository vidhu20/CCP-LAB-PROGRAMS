#include<stdio.h>
int main()
{
  int x, y, z;
  printf("Enter three integer numbers: \n");
  scanf("%d %d %d", &x, &y, &z);
  if((x < y)&&(x < z))
  printf("x is the smallest: %d",x);
  else
  {
    if(y < z)
    printf("y is the smallest: %d",y);
    else
    printf("z is the smallest: %d",z);
    }
  return(0);
}
