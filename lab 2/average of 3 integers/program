#include<stdio.h>
#include<conio.h>
void average3 (int a, int b, int c)
{
	float average;
	average=(a+b+c)/3;
	printf("The average is: %f",average);
}
int main()
{
	int n1,n2,n3;
	void average3 (int a, int b, int c);
	printf("Enter three numbers: \n");
	scanf("%d %d %d", &n1, &n2, &n3);
	average3(n1, n2, n3);
	return 0;
}
